    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "noEmit": false,
  },
  "include": ["*.tsx", "*.spec.tsx"],
